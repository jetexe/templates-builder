{
    "name": "tarampampam/templates-builder",
    "description": "Projects templates builder",
    "keywords": [
        "templates",
        "builder"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Paramtamtam",
            "homepage": "https://github.com/tarampampam/templates-builder"
        }
    ],
    "require": {
        "php": "^5.6 || >=7.0 <7.3",
        "ext-json": "*",
        "ext-mbstring": "*",
        "symfony/console": "~2.7|~3.0",
        "symfony/filesystem": "~2.7|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7 || ^6.4",
        "symfony/var-dumper": "~3.3 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "Tarampampam\\TemplatesBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tarampampam\\TemplatesBuilder\\Tests\\": "tests"
        }
    },
    "bin": [
        "bin/templates-builder"
    ],
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --no-coverage"
    },
    "support": {
        "issues": "https://github.com/tarampampam/templates-builder/issues",
        "source": "https://github.com/tarampampam/templates-builder"
    }
}
